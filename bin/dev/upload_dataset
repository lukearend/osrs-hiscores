#!/usr/bin/env bash
SCRIPT_DIR=$(cd -- "$(dirname -- "${BASH_SOURCE[0]}")" &> /dev/null && pwd)
DATA_FINAL="$SCRIPT_DIR/../../data/processed"

aws s3 cp "$DATA_FINAL/cluster-centroids.csv" "s3://$OSRS_S3_BUCKET/dataset/cluster-centroids.csv"
aws s3 cp "$DATA_FINAL/player-clusters.csv" "s3://$OSRS_S3_BUCKET/dataset/player-clusters.csv"
aws s3 cp "$DATA_FINAL/player-stats.csv" "s3://$OSRS_S3_BUCKET/dataset/player-stats.csv"

gdrive upload --parent "$OSRS_GDRIVE_DIR" --name cluster-centroids.csv "$DATA_FINAL/cluster-centroids.csv"
gdrive upload --parent "$OSRS_GDRIVE_DIR" --name player-clusters.csv "$DATA_FINAL/player-clusters.csv"
gdrive upload --parent "$OSRS_GDRIVE_DIR" --name player-stats.csv "$DATA_FINAL/player-stats.csv"
